    "generateOpenGraphImages:core": "yarn generateOpenGraphImages --core",
    "generateOpenGraphImages:orbit": "yarn generateOpenGraphImages --orbit",
    "generateCoreChainsToMonitor": "ts-node --project ./scripts/tsconfig.json ./scripts/generateCoreChainsToMonitor.ts",
